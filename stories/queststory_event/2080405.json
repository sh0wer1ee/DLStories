            "book_image": ""
        },
        {
            "speaker_id": [
                "110276_01"
            ],
            "speaker_name": "弗里茨",
            "context": "大小姐！？你怎么又……",
            "voice_line": "VO_CHR_EVENTSTORY_20804_05_0018",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110275_01"
            ],
            "speaker_name": "安妮利艾",
            "context": "也谢谢大家！",
            "voice_line": "VO_CHR_EVENTSTORY_20804_05_0019",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100002_01"
            ],
            "speaker_name": "埃尔菲莉丝",
            "context": "什么！？",
            "voice_line": "VO_CHR_EVENTSTORY_20804_05_0020",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100004_01"
            ],
            "speaker_name": "库菈乌",
            "context": "唔。",
            "voice_line": "VO_CHR_EVENTSTORY_20804_05_0022",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110275_01"
            ],
            "speaker_name": "安妮利艾",
            "context": "还有兰扎卜♪",
            "voice_line": "VO_CHR_EVENTSTORY_20804_05_0023",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110276_01"
            ],
            "speaker_name": "弗里茨",
