            "book_image": ""
        },
        {
            "speaker_id": [
                "110367_01"
            ],
            "speaker_name": "少女",
            "context": "你会战斗吗！？",
            "voice_line": "VO_CHR_EVENTSTORY_20423_01_0079",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100009_01"
            ],
            "speaker_name": "塞西娅",
