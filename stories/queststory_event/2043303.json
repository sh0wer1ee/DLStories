            "book_image": ""
        },
        {
            "speaker_id": [
                "110388_01"
            ],
            "speaker_name": "斑比",
            "context": "为、为什么？\\n斑比又没做错什么……",
            "voice_line": "VO_CHR_EVENTSTORY_20433_03_0102",
