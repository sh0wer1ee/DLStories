            "book_image": ""
        },
        {
            "speaker_id": [
                "100054_01"
            ],
            "speaker_name": "塞西娅",
            "context": "所以哥哥就由我来……",
            "voice_line": "VO_CHR_EVENTSTORY_20440_04_0021",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100054_01"
            ],
            "speaker_name": "塞西娅",
