            "book_image": ""
        },
        {
            "speaker_id": [
                "110277_01"
            ],
            "speaker_name": "塔纳托斯",
            "context": "如果你希望如此，我自然会的。\\n来吧，来我这里……",
            "voice_line": "VO_CHR_EVENTSTORY_20406_03_0088",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110277_01"
            ],
            "speaker_name": "？？？",
            "context": "没这个必要！",
            "voice_line": "VO_CHR_EVENTSTORY_20406_03_0089",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120031_01"
            ],
            "speaker_name": "魔兽",
            "context": "咕噢噢噢噢噢！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "菲丽嘉，库菈乌她没事！",
