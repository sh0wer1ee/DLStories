            "book_image": ""
        },
        {
            "speaker_id": [
                "210045_01"
            ],
            "speaker_name": "幽影",
            "context": "谢谢……菲丽嘉，\\n能不能再让我看看你跳的舞？",
            "voice_line": "VO_CHR_EVENTSTORY_20406_07_0096",
