    "story_content": [
        {
            "speaker_id": [
                "120120_01"
            ],
            "speaker_name": "魔兽",
            "context": "咕噜噜噜！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_02"
            ],
            "speaker_name": "勇也",
            "context": "噢啦噢啦！撞飞它！\\n查理奥托！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_02"
            ],
            "speaker_name": "勇也",
            "context": "我可真不得了！\\n一击必杀！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "勇也！\\n就照着这个势头！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_02"
            ],
            "speaker_name": "勇也",
            "context": "遵命！前辈！\\n交给我……诶！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120031_01"
            ],
            "speaker_name": "大型魔兽",
            "context": "咕噜啊啊啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_02"
            ],
            "speaker_name": "勇也",
            "context": "这家伙……不妙啊！？\\n真、真大！牛逼……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "勇也！？你没事吧！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120031_01"
            ],
            "speaker_name": "大型魔兽",
            "context": "咕噜啊啊啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120148_01"
            ],
            "speaker_name": "勇也",
            "context": "不得了！？居然把我打落\\n查理奥托！？等我再坐上……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120031_01"
            ],
            "speaker_name": "大型魔兽",
            "context": "咕噜啊啊啊啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_03"
            ],
            "speaker_name": "勇也",
            "context": "噫！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "怎么了！？你没事吧，勇也！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "呜……不行……\\n可怕……太可怕啦……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "喂喂……勇也那家伙\\n完全被吓懵了啊。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "对、对不起……我……\\n没有查理奥托的话……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "没办法。我们去帮他吧，\\n尤蒂尔！\\n看招！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_07"
            ],
            "speaker_name": "尤蒂尔",
            "context": "哈啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120031_01"
            ],
            "speaker_name": "大型魔兽",
            "context": "咕嘎啊啊啊！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_03"
            ],
            "speaker_name": "勇也",
            "context": "厉害……不愧是前辈们！\\n嘭嘭嘭嘭！一击必杀！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "唔……\\n与其说是我们强……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "其实只是对方太弱了。\\n根本是虚张声势嘛。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "真的假的！？我居然\\n被那种家伙吓懵了……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "泪流满面……我逊毙了……\\n个子又矮……还胆小……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "被红冠救了之后，\\n我就一点都没变……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "红冠？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "没错。\\n你们知道红冠吗？\\n又高大又威猛！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "还有，气势也不同凡响！\\n把我和魔兽都吓傻了！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "虽然不知道你在说些什么……\\n不过大概知道你想表达些什么了。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "嗯，看来有个叫红冠的强者\\n从魔兽群中救出了勇也啊。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "没错！所以我决定了！\\n我要变得高大又威猛！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "我这发型和衣服怎么样！？\\n都是模仿红冠的！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "你说什么！？\\n打扮成这种模样的家伙\\n居然不止你一个！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "没错。红色的红冠\\n有个特大的脑袋……\\n拉风！酷毙了！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "还有，声音也不得了！\\n特别有劲、响亮！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "明明身体那么小……\\n我都怀疑是怎么发声的了！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "嗯？小个子？红冠\\n不是个很高大的家伙吗？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "没错！明明很小却很高大！\\n所以才厉害啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "我不行了……\\n越听越不懂了。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "哈哈……不过，我很理解\\n想成为自己憧憬之人的心情。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "放心吧，勇也总有一天\\n也会变得像那个红冠一样。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "前辈……感激不尽！\\n我立刻就有劲了！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "前辈也总有一天\\n会变得像红冠那样！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "诶、我也！？\\n我、我就算了吧……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "嗯？刚才的声音是……\\n难道又是魔兽！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_03"
            ],
            "speaker_name": "勇也",
            "context": "我、我才不怕！\\n这回一定要魔兽好看！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_03"
            ],
            "speaker_name": "勇也",
            "context": "呀啊啊啊！给我滚出来！\\n魔兽！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120149_01"
            ],
            "speaker_name": "？？？",
            "context": "咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "什么嘛，原来是只鸡啊……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_03"
            ],
            "speaker_name": "勇也",
            "context": "真……真……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "喂喂，勇也，\\n你这也怕过头了……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_03"
            ],
            "speaker_name": "勇也",
            "context": "真的假的！？红冠！\\n为什么会在这里！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01",
                "100003_01"
            ],
            "speaker_name": "二人",
            "context": "诶诶诶诶诶诶！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "你说的红冠……\\n居然不是人，而是只鸡！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "不过……那个大大的鸡冠……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "确实……和勇也很像！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "原、原来如此……\\n可是憧憬的对象竟是只鸡……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "好、好啦。那应该是只强大到\\n能拯救勇也的鸡……吧……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120149_01"
            ],
            "speaker_name": "红冠",
            "context": "咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "果然不同凡响！这气势！\\n真是带劲啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "红冠，太厉害了！\\n今天要教训谁！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120149_01"
            ],
            "speaker_name": "红冠",
            "context": "咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "嗯嗯。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120149_01"
            ],
            "speaker_name": "红冠",
            "context": "咯咯！咯咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "真的假的！？厉害！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "厉害……居然能和鸡对话！\\n它说了什么，勇也？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "我也不知道！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120149_01"
            ],
            "speaker_name": "红冠",
            "context": "咯咯！咯咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "牛逼！不愧是红冠！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "明明听不懂却能够……对话？\\n这到底是怎么回事……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "咯咯咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "兰、兰扎卜！？",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "没有啦，我只是觉得勇也\\n说不定能听懂这个。咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "真的假的！？牛逼啊！\\n兰扎卜前辈！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100003_01"
            ],
            "speaker_name": "兰扎卜",
            "context": "噢？他好像听懂了？\\n咯咯咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "120149_01"
            ],
            "speaker_name": "红冠",
            "context": "咯咯！咯咯咯！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110344_04"
            ],
            "speaker_name": "勇也",
            "context": "厉害！真不愧是前辈们啊！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "这……什么情况……",
            "voice_line": "",
            "book_image": ""
        }
    ]
