            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_01"
            ],
            "speaker_name": "尤蒂尔",
            "context": "啊哈哈哈！",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110047_02"
            ],
            "speaker_name": "伊露缇米娅",
            "context": "……",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [
                "110047_02"
            ],
            "speaker_name": "伊露缇米娅",
            "context": "（不逃避，而是勇于挑战……）",
