    "story_content": [
        {
            "speaker_id": [],
            "speaker_name": "库洛姆",
            "context": "不可以……塞西娅公主，\\n退烧前不要随意走动。",
            "voice_line": "",
            "book_image": ""
        },
        {
            "speaker_id": [],
            "speaker_name": "塞西娅",
