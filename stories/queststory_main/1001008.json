        },
        {
            "speaker_id": [
                "100009_06"
            ],
            "speaker_name": "皇帝塞西娅",
            "context": "哈哈哈哈哈哈哈哈哈！",
            "voice_line": "VO_CHR_MAINSTORY_10_07_0112",
