            "book_image": ""
        },
        {
            "speaker_id": [
                "100009_01"
            ],
            "speaker_name": "塞西娅",
            "context": "哥、哥哥！\\n呀啊啊啊啊啊啊啊啊！",
            "voice_line": "VO_CHR_MAINSTORY_20_12_0028",
            "book_image": ""
        },
        {
            "speaker_id": [
                "100001_09"
